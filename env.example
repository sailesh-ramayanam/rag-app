# Database (configured automatically in Docker)
DATABASE_URL=postgresql+asyncpg://vault_user:vault_password@db:5432/vault_db
SYNC_DATABASE_URL=postgresql://vault_user:vault_password@db:5432/vault_db

# Redis (configured automatically in Docker)
REDIS_URL=redis://redis:6379/0

# Storage path
STORAGE_PATH=/app/storage

# Embedding model (sentence-transformers)
# Options: all-MiniLM-L6-v2 (384 dims), all-mpnet-base-v2 (768 dims)
EMBEDDING_MODEL=all-MiniLM-L6-v2
