# PostgreSQL Configuration
POSTGRES_USER=vault_user
POSTGRES_PASSWORD=vault_password
POSTGRES_DB=vault_db

# Database URLs
DATABASE_URL=postgresql+asyncpg://vault_user:vault_password@db:5432/vault_db
SYNC_DATABASE_URL=postgresql://vault_user:vault_password@db:5432/vault_db

# Redis
REDIS_URL=redis://redis:6379/0

# Storage path
STORAGE_PATH=/app/storage

# Embedding model (sentence-transformers - runs locally)
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
EMBEDDING_DIMENSIONS=384

# LLM Configuration (required for chat feature)
OPENAI_API_KEY=your_openai_api_key_here

# LLM Provider and Model
LLM_PROVIDER=openai
LLM_MODEL=gpt-5-nano
